{
  "name": "highlight.js-tsql",
  "description": "Syntax highlighting for T-SQL.",
  "keywords": [
    "highlight",
    "syntax",
    "T-SQL",
    "sql"
  ],
  "homepage": "https://github.com/highlightjs/highlightjs-tsql/",
  "version": "1.0.4",
  "author": {
    "name": "highlight.js contributors and Redgate Software",
    "email": "support@red-gate.com"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/highlightjs/highlightjs-tsql/issues"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/highlightjs/highlightjs-tsql.git"
  },
  "main": "./lib/index.js",
  "scripts": {
    "mocha": "mocha",
    "test": "mocha --globals document test",
    "test-browser": "mocha --globals document test/browser",
    "build": "node tools/build.js tsql",
    "postbuild": "cpy ./build/highlight.pack.js ./dist/ --rename=highlight.min.js && cpy ./build/demo/styles/ssms.css ./dist/ --rename=ssms.min.css",
    "prepare": "npm run build"
  },
  "files": [
    "dist/highlight.min.js",
    "dist/ssms.min.css"
  ],
  "engines": {
    "node": "*"
  },
  "devDependencies": {
    "bluebird": "^3.5.5",
    "commander": "^3.0.1",
    "cpy-cli": "^1.0.1",
    "del": "^5.1.0",
    "gear": "github:highlightjs/gear",
    "gear-lib": "github:highlightjs/gear-lib",
    "glob": "^7.1.4",
    "js-beautify": "^1.10.2",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "should": "^13.2.3",
    "tiny-worker": "^2.3.0"
  }
}
